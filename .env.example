# =============================================
# Shinhan AAS Server 환경변수 설정
# 이 파일을 복사하여 .env 파일을 만들어 사용하세요
# cp .env.example .env
# =============================================

# 프로필 설정 (oracle, mysql, default)
SPRING_PROFILES_ACTIVE=oracle

# 서버 포트
SERVER_PORT=6443

# Oracle Database 설정
SPRING_DATASOURCE_URL=jdbc:oracle:thin:@localhost:1521:ORCL
SPRING_DATASOURCE_USERNAME=aas_user
SPRING_DATASOURCE_PASSWORD=your_password_here

# JWT 설정 (운영환경에서는 반드시 변경)
JWT_SECRET=shinhan-aas-server-secret-key-must-be-at-least-256-bits-long-for-hs256

# JPA 설정
SPRING_JPA_HIBERNATE_DDL_AUTO=validate
SPRING_JPA_SHOW_SQL=false
